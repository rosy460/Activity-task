#write python function to find maximum of three numbers.
def find_max(num1, num2,num3):
    return max (num1, num2,num3)
print(find_max(8,3,10))

#write a python program to reverse a string.
text = "hello! my name is rameen"
reverse_text=""
for char in text:
    reversed_text = char + reverse_text
    print(reverse_text)

#create and print a list of squares for numbers 1 to 30.
def get_squares():
    squares=[]
    for i in range(1,30):
        squares.append(i*i)
        return squares
    print("list of squares from 1 to 30:")
    print(get_squares())
